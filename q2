#include <iostream>
#include <string.h>
using namespace std;

int main() {
    char s[200];
    cout << "Enter string (no spaces): ";
    cin >> s;

    int n = strlen(s);
    char st[200];
    int top = -1;

    // push all characters
    for (int i = 0; i < n; i++) st[++top] = s[i];

    // pop to build reverse
    cout << "Reversed: ";
    while (top != -1) cout << st[top--];
    cout << "\n";
    return 0;
}
